#!/usr/bin/env php
<?php
set_time_limit(0);

require __DIR__.'/../vendor/autoload.php';

// setup Application
$app  = new App\Application('console');

// Init the app
$app->init();

// instance of console
/** @var Knp\Console\Application $console */
$console = $app['console'];

// run console
$console->run();
